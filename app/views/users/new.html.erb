<%= form_tag "/users" do %>
  <%= label_tag :username %>
  <%= text_field_tag :username %>

  <%= label_tag :email %>
  <%= text_field_tag :email %>

  <%= label_tag :password %>
  <%= text_field_tag :password %>

  <%= label_tag :password_confirmation %>
  <%= text_field_tag :password_confirmation %>

  <%= submit_tag "Create Account" %>
<% end %>
